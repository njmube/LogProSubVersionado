<WMCore:UserControlBase x:Uid="KitAssembly" x:Class="WpfFront.Views.KitAssemblyView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:xcdg="http://schemas.xceed.com/wpf/xaml/datagrid"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="Auto" Width="Auto" HorizontalAlignment="Stretch"
    xmlns:odc="clr-namespace:Odyssey.Controls;assembly=Odyssey"
    xmlns:WMCore="clr-namespace:Core.WPF;assembly=Core" 
    xmlns:uc="clr-namespace:WpfFront.Common.UserControls"
    x:Name="KitAssembly">
    <AdornerDecorator x:Uid="AdornerDecorator_1">
        <AdornerDecorator.Resources>
            <xcdg:DataGridCollectionViewSource x:Uid="xcdg:DataGridCollectionViewSource_1" x:Key="ListDoc" Source="{Binding DocumentList}" AutoFilterMode="And"  DefaultCalculateDistinctValues="True"  />
        </AdornerDecorator.Resources>
        
        <DockPanel x:Uid="DockPanel_1" Background="{DynamicResource WBg}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">

            <DockPanel.Resources>
                <WMCore:ForceReReadConverter x:Uid="WMCore:ForceReReadConverter_1" x:Key="forceReReadConverter" />
                <WMCore:FormattingConverter x:Uid="WMCore:FormattingConverter_1" x:Key="formattingConverter" />
                <WMCore:FormNotificationErrorMessageConverter x:Uid="WMCore:FormNotificationErrorMessageConverter_1" x:Key="formNotificationErrorMessageConverter" />
                <WMCore:NullableIntegerFormatConverter x:Uid="WMCore:NullableIntegerFormatConverter_1" x:Key="nullableIntegerFormatConverter" />
            </DockPanel.Resources>

           
            <!-- Tab Control, 
            Kit/Assembly Orders,
                - Lista de ordenes y su stuts
                - Picking de la orden (print new labels)
                - Create assembli order
            Admin Formulas 
                - Add Remove product from formula
                - Create new formaulas
            -->
           
                        <!-- COLUMN ONE -->

                        <!-- PANEL DE LISTA DE DOCUMENTOS -->


                        <StackPanel x:Uid="StackPanel_1" DockPanel.Dock="Left" Orientation="Vertical" Width="Auto" Height="Auto"  Margin="0,0,0,0" VerticalAlignment="Stretch">

                    <TextBlock x:Uid="TextBlock_1" DockPanel.Dock="Top" Margin="4" FontSize="14" FontWeight="Bold" HorizontalAlignment="Center" Text="{StaticResource KIT_ASSEMBLY_ORDERS}"></TextBlock>


                    <StackPanel x:Uid="stpDocList2" Name="stpDocList2">

                                <Border x:Uid="Border_1" Style="{DynamicResource UserControlFormInnerBorder}"  
                        HorizontalAlignment="Stretch"  VerticalAlignment="Stretch">

                                    <Grid x:Uid="Grid_1" Margin="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition x:Uid="RowDefinition_1" Height="auto" />
                                            <RowDefinition x:Uid="RowDefinition_2" Height="auto" />
                                        </Grid.RowDefinitions>


                                        <StackPanel x:Uid="StackPanel_2" Orientation="Horizontal" Grid.Row="0">
                                            <TextBlock x:Uid="TextBlock_2" VerticalAlignment="Center" Text="{StaticResource SEARCH_DOCUMENT}"></TextBlock>
                                            <TextBox x:Uid="txtSearch" Width="100" x:Name="txtSearch" Margin="4,2,0,4" ToolTip="Enter keyword to search." TextChanged="txtSearch_TextChanged" />
                                            <CheckBox x:Uid="chkLate" HorizontalAlignment="Stretch" x:Name="chkLate" Margin="30,9,0,4" Width="Auto"  VerticalContentAlignment="Top" Checked="chkLate_Checked" Unchecked="chkLate_Checked" Content="{StaticResource LATE_DOCUMENTS}"></CheckBox>
                                    <Image x:Uid="Image_1" Margin="10,0,0,0" Width="17" HorizontalAlignment="Center" Source="/WpfFront;component/Images/refresh.png" MouseDown="Image_MouseDown" ToolTip="Refresh List" />


                                </StackPanel>

                                        <!-- Chose Columns to show -->



                                        <xcdg:DataGridControl  x:Uid="dgDocument" x:Name="dgDocument"  Grid.Row="1" ItemsSource="{Binding Source={StaticResource ListDoc}}" 
                                          AutoCreateColumns="False" Width="Auto" ReadOnly="False" 
                                          MaxHeight="450" Margin="0,0,0,0" VerticalAlignment="Top" SelectionMode="Single" 
                                                          NavigationBehavior="RowOnly" GotFocus="dgDocument_Click"
                                            ItemScrollingBehavior="Immediate" >

                                            <xcdg:DataGridControl.Resources>
                                                <Style x:Uid="Style_1" x:Key="{x:Type xcdg:ScrollTip}" TargetType="{x:Type xcdg:ScrollTip}">
                                                    <Setter x:Uid="Setter_1" Property="HorizontalAlignment" Value="Right" />
                                                    <Setter x:Uid="Setter_2" Property="VerticalAlignment" Value="Bottom" />
                                                </Style>
                                            </xcdg:DataGridControl.Resources>

                                            <xcdg:DataGridControl.ContextMenu>
                                                <ContextMenu x:Uid="ctnColsMenu1" Name="ctnColsMenu1">
                                                    <MenuItem x:Uid="MenuItem_1" Header="Show/Hide Columns" Tag="CustomizeColumns" MouseEnter="MenuItem_MouseEnter"/>
                                                </ContextMenu>
                                            </xcdg:DataGridControl.ContextMenu>


                                            <xcdg:DataGridControl.Columns>
                                                <!--<xcdg:Column FieldName="Priority" Title="Prio" Width="25" />-->
                                                <xcdg:Column x:Uid="xcdg:Column_1" FieldName="DocNumber" IsMainColumn="True" Title="{StaticResource DOCUMENT}" Width="115" ReadOnly="True"  />
                                                <!--<xcdg:Column FieldName="Vendor.Name" Title="Vendor"  Width="120" TextWrapping="Wrap" ReadOnly="True"  />-->
                                                <xcdg:Column x:Uid="xcdg:Column_2" FieldName="Date1" Title="{StaticResource ORDER_DATE}"  Width="65" ReadOnly="True" />
                                                <xcdg:Column x:Uid="xcdg:Column_3" FieldName="Date2" Title="{StaticResource REQUIRED_ON}" Visible="False"  Width="65" ReadOnly="True" />
                                                <xcdg:Column x:Uid="xcdg:Column_4" FieldName="DocStatus.Name" Title="{StaticResource STATUS}" Width="50"  ReadOnly="True" />
                                                <xcdg:Column x:Uid="xcdg:Column_5" FieldName="Reference" Title="{StaticResource REFERENCE}" Width="60" Visible="False"  ReadOnly="True" />

                                                <xcdg:Column x:Uid="xcdg:Column_6" FieldName="Date3" Title="{StaticResource PROMISED_ON}" Width="65" Visible="False"  ReadOnly="True" />
                                                <xcdg:Column x:Uid="xcdg:Column_7" FieldName="DocType.Name" Title="{StaticResource TYPE}" Width="60" Visible="False"  ReadOnly="True" />
                                                <xcdg:Column x:Uid="xcdg:Column_8" FieldName="Comment" Title="{StaticResource COMMENT}" Width="90" Visible="True"  ReadOnly="True" TextWrapping="Wrap" />
                                                <xcdg:Column x:Uid="xcdg:Column_9" FieldName="AssignedUsers" Title="{StaticResource PICKERS}" Width="90" Visible="False"  TextWrapping="Wrap" ReadOnly="True" />

                                            </xcdg:DataGridControl.Columns>
                                            <xcdg:DataGridControl.View>
                                                <xcdg:TableView x:Uid="xcdg:TableView_1" UseDefaultHeadersFooters="False" ShowScrollTip="True" VerticalGridLineBrush="LightGray" ShowRowSelectorPane="False" HorizontalGridLineThickness="1" HorizontalGridLineBrush="LightGray">
                                                    <xcdg:TableView.Theme>
                                                        <xcdg:Office2007BlackTheme x:Uid="xcdg:Office2007BlackTheme_1"/>
                                                    </xcdg:TableView.Theme>
                                                    <xcdg:TableView.FixedHeaders>
                                                        <DataTemplate x:Uid="DataTemplate_1">
                                                            <xcdg:HierarchicalGroupByControl x:Uid="xcdg:HierarchicalGroupByControl_1" xcdg:TableView.CanScrollHorizontally="False" />
                                                        </DataTemplate>
                                                        <DataTemplate x:Uid="DataTemplate_2">
                                                            <xcdg:ColumnManagerRow x:Uid="xcdg:ColumnManagerRow_4" AllowAutoFilter="True" AllowColumnReorder="True" />
                                                        </DataTemplate>

                                        </xcdg:TableView.FixedHeaders>
                                                </xcdg:TableView>
                                            </xcdg:DataGridControl.View>
                                        </xcdg:DataGridControl>
                                    </Grid>
                                </Border>
                                <!--<StackPanel Visibility="Hidden" x:Name="pgrBar" DockPanel.Dock="Bottom" Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Bottom">
                    <TextBlock Name="StatusText" Margin="10,0,5,0"  VerticalAlignment="Center">Processing</TextBlock> 
                    <ProgressBar Name="Progress" IsIndeterminate="True" Width="100" Height="15" Minimum="0" Maximum="1" Margin="5" />                    
                    </StackPanel>-->

                                <!--</StackPanel>
                            </odc:OutlookSection.Content>
                        </odc:OutlookSection>
                    </odc:OutlookBar.Sections>
                </odc:OutlookBar>-->
                                <!--</Grid>-->
                            </StackPanel>



                            <!--</Expander>-->


                            <!--</DockPanel>-->



                            <!-- END PANEL DE LISTA DE DOCUMENTOS -->
                        </StackPanel>



                        <TabControl x:Uid="tabMenu" Margin="2,5,5,0" Name="tabMenu" DockPanel.Dock="Top"
                        Style="{DynamicResource TabControlStyle}" Visibility="Hidden" HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch">

                            <TabItem x:Uid="TabItem_1" Header="{StaticResource DOCUMENT_DETAILS}">

                                <StackPanel x:Uid="StackPanel_3" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="3">


                                    <Border  x:Uid="Border_2" Style="{DynamicResource UserControlFormInnerBorder}" DockPanel.Dock="Bottom" 
                    HorizontalAlignment="Stretch"  VerticalAlignment="Stretch" >

                                        <StackPanel x:Uid="StackPanel_4">
                                            <StackPanel x:Uid="StackPanel_5" Orientation="Horizontal" Margin="2">
                                                <StackPanel x:Uid="StackPanel_6" Orientation="Vertical">

                                                    <StackPanel x:Uid="StackPanel_7" Orientation="Horizontal">
                                                        <TextBlock x:Uid="TextBlock_3" FontWeight="Bold" Text="{StaticResource CURRENT_DOCUMENT}"></TextBlock>

                                                        <TextBlock x:Uid="TextBlock_4" FontWeight="Bold" Margin="5,0,0,0" Text="{Binding Path=Document.DocNumber}" Foreground="Brown"></TextBlock>
                                                    </StackPanel>

                                                    <ListView x:Uid="lvDocumentData" ItemsSource="{Binding Path=DocumentData}"  Name="lvDocumentData" MaxHeight="100" MaxWidth="240"  >
                                                        <ListView.View>
                                                            <GridView x:Uid="GridView_1" ColumnHeaderContainerStyle="{DynamicResource CustomHeaderStyle}">
                                                                <GridViewColumn x:Uid="GridViewColumn_1" Header="{StaticResource KEY}" DisplayMemberBinding="{Binding Path=DataKey}" Width="40"></GridViewColumn>

                                                                <GridViewColumn x:Uid="GridViewColumn_2" Header="{StaticResource VALUE}" Width="120" >
                                                                    <GridViewColumn.CellTemplate>
                                                                        <DataTemplate x:Uid="DataTemplate_3">
                                                                            <TextBlock x:Uid="TextBlock_5" Text="{Binding Path=DataValue}" TextWrapping="Wrap" />
                                                                        </DataTemplate>
                                                                    </GridViewColumn.CellTemplate>
                                                                </GridViewColumn>

                                                            </GridView>
                                                        </ListView.View>
                                                    </ListView>
                                                </StackPanel>

                                                <StackPanel x:Uid="StackPanel_8" Margin="15,0,0,0">

                                                    <WrapPanel x:Uid="WrapPanel_1" Margin="0,0,0,5">

                                                        <Image x:Uid="Image_1" Margin="0,0,4,0" Width="20" HorizontalAlignment="Center" 
                                               Source="/WpfFront;component/Images/Icons/Cross.png" Name="xRefresh"
                                               MouseDown="xRefresh_MouseDown" ToolTip="Refresh Document" />
                                                        
                                                        <Button x:Uid="btnRecTkt" HorizontalAlignment="Left" Name="btnRecTkt" MinHeight="30" 
                                            Margin="0,0,6,0" Click="btnRecTkt_Click" Width="110">
                                                            <StackPanel x:Uid="StackPanel_9" Orientation="Horizontal">
                                                                <Image x:Uid="Image_2" Margin="5,0,0,0"  Width="18" Source="/WpfFront;component/Images/invoice.jpg"></Image>
                                                                <TextBlock x:Uid="TextBlock_6" Margin="5" Text="{StaticResource ORDER_TICKET}"></TextBlock>
                                                            </StackPanel>
                                                        </Button>

                                                        
                                                       <uc:DirectPrint x:Name="ucDirectPrint" Margin="0,0,5,0"  />
                                                        
                                                        
                                                        <Button x:Uid="btnPrintOpc" HorizontalAlignment="Left" Name="btnPrintOpc" MinHeight="30" 
                                            Margin="0,0,6,0" Click="btnPrintOpc_Click"  IsEnabled="{Binding Path=AllPicked}" >
                                                            <StackPanel x:Uid="StackPanel_10" Orientation="Horizontal" >
                                                                <Image x:Uid="Image_3" Margin="5,0,0,0"  Width="16" Source="/WpfFront;component/Images/label.jpg"></Image>
                                                                <TextBlock x:Uid="TextBlock_7" Margin="5" Text="{StaticResource PRINT_LABELS1}"></TextBlock>
                                                            </StackPanel>
                                                        </Button>


                                                    </WrapPanel>

                                                    <Separator x:Uid="Separator_1" />

                                                    <StackPanel x:Uid="StackPanel_11" Orientation="Vertical">

                                                        <TextBlock x:Uid="TextBlock_8" FontWeight="Bold" Text="{StaticResource NOTES_COMMENTS}"></TextBlock>
                                                        <TextBox x:Uid="TextBox_1" TextWrapping="Wrap" Height="40" HorizontalAlignment="Left" Width="345"   Text="{Binding Path=Document.Comment}"></TextBox>


                                                <Grid x:Uid="Grid_2" Margin="0,5,0,0">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition x:Uid="RowDefinition_3" Height="auto" />
                                                        <RowDefinition x:Uid="RowDefinition_4" Height="auto" />
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition x:Uid="ColumnDefinition_1" Width="Auto" />
                                                        <ColumnDefinition x:Uid="ColumnDefinition_2" Width="Auto" />
                                                        <ColumnDefinition x:Uid="ColumnDefinition_3" Width="Auto" />
                                                    </Grid.ColumnDefinitions>

                                                    
                                                    <TextBlock x:Uid="TextBlock_9" Grid.Column="0" Grid.Row="0" Margin="5,0,0,0" VerticalAlignment="Center" FontWeight="Bold" Text="{StaticResource STATUS}"></TextBlock>

                                                    <ComboBox x:Uid="cboStatus" Grid.Column="0"  Grid.Row="1" x:Name="cboStatus" ItemsSource="{Binding Path=DocStatus}" DisplayMemberPath="Name" Margin="5,0,0,0"
                                                  SelectedValuePath="StatusID"  SelectedValue="{Binding Path=Document.DocStatus.StatusID}" Width="80" ToolTip="In Process: Partially Picked, Completed: Full Picked before send to ERP, Posted: Confirmed in ERP." />


                                                    <TextBlock x:Uid="TextBlock_10" Grid.Column="1" Grid.Row="0" Margin="8,0,0,0"  VerticalAlignment="Center" FontWeight="Bold" Text="{StaticResource PICKING_METHOD}"></TextBlock>

                                                    <ComboBox x:Uid="cboPickM" Grid.Column="1" Grid.Row="1" x:Name="cboPickM"  ItemsSource="{Binding Path=PickMethods}" 
                                                              DisplayMemberPath="Name" Margin="8,0,0,0"
                                                              SelectedValuePath="MethodID" Width="100" SelectedValue="{Binding Path=Document.PickMethod.MethodID}" />



                                                    <Button x:Uid="btnChangeStatus" Grid.Column="2" Grid.RowSpan="2" Width="65" VerticalAlignment="Bottom"  x:Name="btnChangeStatus"  FontWeight="Bold" Click="btnChangeStatus_Click" Margin="8,0,0,0" Content="{StaticResource UPDATE}" ></Button>
                                                </Grid>
                                                        
                                            </StackPanel>

                                                </StackPanel>


                                            </StackPanel>

                                        </StackPanel>

                                    </Border>

                            <StackPanel x:Uid="StackPanel_12" Margin="5,3,8,6" Orientation="Horizontal">
                                <TextBlock x:Uid="TextBlock_11" FontSize="12" FontWeight="Bold" Text="{StaticResource ITEM_TO_BUILD}"></TextBlock>
                                <TextBlock  x:Uid="TextBlock_12" FontSize="12" Foreground="Brown" FontWeight="Bold" Margin="5,0,0,0"  Text="{Binding Path=KitProduct.FullDesc}"></TextBlock>
                                <!--<TextBlock  x:Uid="TextBlock_13" FontSize="12" Foreground="Brown" FontWeight="Bold" Margin="5,0,0,0"  Text="{Binding Path=KitProduct.Name}"></TextBlock>-->
                            </StackPanel>
                                    
                                    <odc:OdcExpander  x:Uid="expDocLines" Header="{StaticResource DOCUMENT_LINES}" Margin="5,5,8,6" Background="White"  
                               IsMinimized="False"  IsExpanded="False"  ShowEllipse="False" x:Name="expDocLines" >

                                        <Grid x:Uid="Grid_3">
                                            <Grid.Resources>
                                                <Style x:Uid="Style_2" TargetType="{x:Type xcdg:DataRow}">
                                                    <Style.Triggers>
                                                        <DataTrigger x:Uid="DataTrigger_1" Binding="{Binding Path=LineStatus.Name}"  Value="Cancelled">
                                                            <Setter x:Uid="Setter_3" Property="Background" Value="#FCA2A2"/>
                                                        </DataTrigger>

                                                        <DataTrigger x:Uid="DataTrigger_2" Binding="{Binding Path=Note}" Value="2">
                                                            <Setter x:Uid="Setter_4" Property="Background" Value="#FCFE70"/>
                                                        </DataTrigger>

                                                    </Style.Triggers>
                                                </Style>
                                            </Grid.Resources>

                                            <xcdg:DataGridControl  x:Uid="dgLines" Margin="-3,-3,-3,0" x:Name="dgLines"  ItemsSource="{Binding Path=DocumentLines}" 
                                HorizontalAlignment="Stretch" AutoCreateColumns="False" ReadOnly="True" MaxHeight="260">
                                                <xcdg:DataGridControl.Resources>
                                                    <Style x:Uid="Style_3" x:Key="{x:Type xcdg:ScrollTip}" TargetType="{x:Type xcdg:ScrollTip}">
                                                        <Setter x:Uid="Setter_5" Property="HorizontalAlignment" Value="Right" />
                                                        <Setter x:Uid="Setter_6" Property="VerticalAlignment" Value="Bottom" />
                                                    </Style>
                                                </xcdg:DataGridControl.Resources>
                                                <xcdg:DataGridControl.Columns>
                                                    <!--<xcdg:Column FieldName="LineNumber" IsMainColumn="True" Title="ComponentID" Width="80" />-->
                                                    <!--<xcdg:Column FieldName="LineStatus.Name" Title="Status" Width="60" />-->
                                                    <xcdg:Column x:Uid="xcdg:Column_10" FieldName="Product.ProductCode" Title="{StaticResource COMPONENT}"  Width="110"/>
                                                    <xcdg:Column x:Uid="xcdg:Column_11" FieldName="Product.Description" Title="{StaticResource DESCRIPTION}" Width="200" TextWrapping="Wrap" />
                                                    <xcdg:Column x:Uid="xcdg:Column_12" FieldName="Unit.Name" Title="{StaticResource UOM}" Width="60" />
                                                    <xcdg:Column x:Uid="xcdg:Column_13" FieldName="Quantity" Title="{StaticResource QUANTITY}" Width="60" />
                                                    <xcdg:Column x:Uid="xcdg:Column_14" FieldName="LinkDocLineNumber" IsMainColumn="True" Title="{StaticResource PARENTID}" Width="50" />
                                                    <xcdg:Column x:Uid="xcdg:Column_15" FieldName="KitNote" Title="" Width="90" />
                                        </xcdg:DataGridControl.Columns>
                                                <xcdg:DataGridControl.View>
                                                    <xcdg:TableView x:Uid="xcdg:TableView_2" UseDefaultHeadersFooters="False" ShowScrollTip="False" VerticalGridLineBrush="Beige" ShowRowSelectorPane="False" ShowFixedColumnSplitter="False">
                                                        <xcdg:TableView.Theme>
                                                            <xcdg:Office2007BlackTheme x:Uid="xcdg:Office2007BlackTheme_2"/>
                                                        </xcdg:TableView.Theme>
                                                        <xcdg:TableView.FixedHeaders>
                                                            <!--<DataTemplate x:Uid="DataTemplate_4">
                                                                <xcdg:HierarchicalGroupByControl x:Uid="xcdg:HierarchicalGroupByControl_2" xcdg:TableView.CanScrollHorizontally="False" />
                                                            </DataTemplate>-->
                                                            <DataTemplate x:Uid="DataTemplate_5">
                                                                <xcdg:ColumnManagerRow x:Uid="xcdg:ColumnManagerRow_2" />
                                                            </DataTemplate>
                                                        </xcdg:TableView.FixedHeaders>
                                                    </xcdg:TableView>
                                                </xcdg:DataGridControl.View>
                                            </xcdg:DataGridControl>
                                        </Grid>



                                    </odc:OdcExpander>

                                    <!-- END CURRENT DOCUMENT LINE DETAILS  -->



                                </StackPanel>

                            </TabItem>


                            <!-- Receiving -->
                            <TabItem x:Uid="tbItemReceive" Header="{StaticResource COMPONENT_PICKING}" HorizontalAlignment="Stretch" x:Name="tbItemReceive">

                                <!-- RECEIVING BOX -->

                                <StackPanel   x:Uid="stkBox" x:Name="stkBox"  Orientation="Vertical" Margin="3">
                                    <!-- <TextBlock Text="User Validations" TextWrapping="Wrap" Foreground="{DynamicResource Brush_FormHeadingText}" FontWeight="Bold" FontSize="14" Margin="10,0,0,0" /> -->
                                    <WMCore:FormNotification x:Uid="frmNotification" IsTabStop="False" x:Name="frmNotification" Margin="0,0,0,0" Height="1"
                                         Panel.ZIndex="99" AutoCollapseTimeout="2">
                                        <WMCore:FormNotification.ErrorMessage>
                                            <MultiBinding x:Uid="MultiBinding_1" Converter="{StaticResource formNotificationErrorMessageConverter}">
                                                <Binding x:Uid="Binding_1" Path="Error" />
                                                <Binding x:Uid="Binding_2" Path="ValidationExceptionErrors" ElementName="KitAssembly" />
                                            </MultiBinding>
                                        </WMCore:FormNotification.ErrorMessage>
                                    </WMCore:FormNotification>


                                    <!-- Bin Destination -->

                                    <Border  x:Uid="Border_3" Style="{DynamicResource UserControlFormInnerBorder}">
                                        <Grid x:Uid="Grid_4">
                                            <StackPanel x:Uid="StackPanel_13" Orientation="Horizontal" Margin="0,0,0,0" HorizontalAlignment="Left">
                                                
                                                <TextBlock  x:Uid="TextBlock_14" Margin="0,0,10,0" VerticalAlignment="Center"  HorizontalAlignment="Right" FontWeight="Bold" Text="{StaticResource ENT_BIN_LAB_SOU}"></TextBlock>

                                                <TextBox x:Uid="txtBinLabel" Width="120" ToolTip="Scan or write a bin or label location."  LostFocus="txtBinLabel_LostFocus"
                                 Margin="0,0,0,0" Height="22" Name="txtBinLabel" />
                                                
                                                <!--


                                                 Listado de Bines to select 
                                                <ComboBox Width="100" IsEditable="True" Height="25" StaysOpenOnEdit="True" Name="cboBin" DisplayMemberPath="BinCode"
                                         ItemsSource="{Binding Path=BinList}" Visibility="Collapsed" SelectionChanged="cboBin_SelectionChanged" />-->
                                   
                                    <TextBlock x:Uid="txtProcessResult" x:Name="txtProcessResult" TextWrapping="Wrap" Margin="8,3,0,0" 
                                               Style="{DynamicResource ProcessResult}"  />


                                        <Button x:Uid="btnConfirmAsemblyOrder" FontWeight="Bold"  Grid.Column="2" Margin="30,0,0,0" Width="110" Height="30" ToolTip="Confirm the adjustment to add new items and decrease kit/assembly components."
                                                 x:Name="btnConfirmAsemblyOrder" IsEnabled="{Binding Path=AllPicked}" VerticalAlignment="Bottom" Content="{StaticResource CONFIRM_ORDER}" Click="btnConfirmAsemblyOrder_Click">
                                        </Button> <!--   -->

                                        <Button x:Uid="btnPrintOpc2" HorizontalAlignment="Left" Name="btnPrintOpc2" MinHeight="30" Visibility="Collapsed"
                                            Margin="10,0,0,0" Click="btnPrintOpc_Click" IsEnabled="{Binding Path=AllPicked}">
                                            <StackPanel x:Uid="StackPanel_14" Orientation="Horizontal" >
                                                <Image x:Uid="Image_4" Margin="0,0,0,0"  Width="16" Source="/WpfFront;component/Images/label.jpg"></Image>
                                                <TextBlock x:Uid="TextBlock_15" Margin="5" Text="{StaticResource PRINT_LABELS1}"></TextBlock>
                                            </StackPanel>
                                        </Button>

                                    </StackPanel>
                                        </Grid>
                                    </Border>

                                    <!-- Receive  Manual -->
                                    <odc:OdcExpander x:Uid="expManual" x:Name="expManual" Header="{StaticResource PICKING_MANUAL}" Margin="5,0,8,0" Background="White"  
                               ShowEllipse="False" Expanded="expLabel_Expanded" Collapsed="expLabel_Collapsed">


                                        <StackPanel x:Uid="StackPanel_15" Orientation="Horizontal" Margin="0">
                                            <!-- Enter Manual -->
                                            <Grid  x:Uid="dgReceive" HorizontalAlignment="Left" Width="Auto"
                            VerticalAlignment="Top"  Margin="-3,-3, -3, 0" x:Name="dgReceive" >

                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition x:Uid="ColumnDefinition_4" Width="Auto" />
                                                    <ColumnDefinition x:Uid="ColumnDefinition_5" Width="Auto" />
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition x:Uid="RowDefinition_5" Height="auto" />
                                                    <RowDefinition x:Uid="RowDefinition_6" Height="auto" />
                                                    <RowDefinition x:Uid="RowDefinition_7" Height="auto" />
                                                    <RowDefinition x:Uid="RowDefinition_8" Height="auto" />
                                                    <RowDefinition x:Uid="RowDefinition_9" Height="auto" />
                                                    <RowDefinition x:Uid="RowDefinition_10" Height="auto" />
                                                    <RowDefinition x:Uid="RowDefinition_11" Height="auto" />
                                                </Grid.RowDefinitions>

                                                <TextBlock x:Uid="TextBlock_16" Grid.Column="0" Grid.Row="1" Margin="5,4,0,0" HorizontalAlignment="Right" Text="{StaticResource COMPONENT}"></TextBlock>


                                                <StackPanel x:Uid="StackPanel_16" Grid.Column="1" Grid.Row="1" Margin="5,0,0,0" HorizontalAlignment="Left">

                                        <uc:SearchProduct x:Uid="txtProduct" x:Name="txtProduct" OnLoadRecord="txtProduct_OnLoadRecord" />

                                        <!--<TextBox  Name="txtProduct"  Margin="0,0,0,0" TextChanged="txtProduct_TextChanged" HorizontalAlignment="Left" Width="120" />

                                                    <ListView ItemsSource="{Binding Path=DocProducts}" SelectionChanged="cboProduct_SelectionChanged" 
                                      Name="cboProduct" Height="Auto" MaxHeight="60" Margin="0">

                                                        <ListView.View>
                                                            <GridView  ColumnHeaderContainerStyle="{DynamicResource CustomHeaderStyle}">
                                                                <GridViewColumn Header="Product" Width="100" DisplayMemberBinding="{Binding Path=ProductCode}"></GridViewColumn>
                                                                <GridViewColumn Header="Name" Width="120" DisplayMemberBinding="{Binding Path=Name}"></GridViewColumn>
                                                            </GridView>
