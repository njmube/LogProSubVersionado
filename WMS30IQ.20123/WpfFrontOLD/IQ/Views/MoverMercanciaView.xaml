                                            <Button x:Uid="btn_ActualizarCambioClasificacion" Name="btn_ActualizarCambioClasificacion" Height="23" Margin="8,0,0,0" Content="Actualizar" Click="btn_ActualizarCambioClasificacion_Click_1" />
                                        </StackPanel>
                                    </StackPanel>
                                    <StackPanel x:Uid="Stack_Adicionales_2" x:Name="Stack_Adicionales_1" Orientation="Vertical" Margin="0,0,0,0">
                                        <ListView x:Uid="lv_ListadoBusquedaCambioClasificacion" SelectionMode="Extended" ItemsSource="{Binding Path=ListadoCambioClasificacion}" Margin="0,4,0,0"  MaxHeight="400" x:Name="lv_ListadoBusquedaCambioClasificacion" SelectionChanged="lv_ListadoBusquedaCambioClasificacion_SelectionChanged_1">
                                            <ListView.View>
