                                        </StackPanel>
                                        <StackPanel x:Uid="stkData" Orientation="Vertical" Margin="7,4,4,4">
                                            <TextBlock x:Uid="TextBlock_4" FontWeight="Bold" Margin="0,4,4,0" HorizontalAlignment="Left" Text="Modelo" />
