                                    <Separator Margin="5,5,0,10"/>
                                    <StackPanel x:Uid="Stack_Panel2" Width="auto" Orientation="Horizontal" Margin="4,3,4,3">
                                        <TextBlock x:Uid="Text_block2" Width="150" Text="Destino" VerticalAlignment="Center" Margin="4,0,6,0" />
                                        <ComboBox x:Uid="cb_Ubicacion" Name="cb_Ubicacion" SelectionChanged="cb_CambioDestino" Width="179" Height="23" ItemsSource="{Binding Path=ListUbicacionesDestino}" SelectedValue="UbicacionDestino" DisplayMemberPath="UbicacionDestino" />
                                    </StackPanel>

                                    <StackPanel x:Uid="Stack_Panel2" x:Name="stack_nuevaUbicacion" Width="auto" Orientation="Horizontal" Margin="4,3,4,3" Visibility="Collapsed">
                                        <TextBlock x:Uid="Text_block2" Width="150" Text="Ubicacion almacenamiento" VerticalAlignment="Center" Margin="4,0,6,0" />
                                        <ComboBox x:Uid="cb_NuevaUbicacion" x:Name="cb_NuevaUbicacion" Width="179" Height="23" ItemsSource="{Binding Path=ListadoPosicionesCambioUbicacion}" SelectedValue="Code" DisplayMemberPath="Name" />
                                    </StackPanel>

                              
                                    <StackPanel x:Uid="Stack_Panel_2" Name="Stack_Panel_2" Width="auto" Orientation="Horizontal" Margin="4,3,11,3" HorizontalAlignment="Right" >
                                        <Button x:Uid="btn_imprimir" Name="btn_imprimir" Content="Imprimir" Click="btn_imprimirHablador_Click_1" Margin="0,0,5,0" />
                                        <Button x:Uid="btn_confirmar_2" Name="btn_confirm" Content="Empacar" Click="btn_confirmar_Click_1" />
                                    </StackPanel>
                                    <ListView x:Uid="lvDocumentMaster" ItemsSource="{Binding Path = ListRecordsAddToPallet}" Margin="0,4,0,0" MaxHeight="400" Name="lvDocumentMasterAddToPallet">
                                        <ListView.View>
                                            <GridView x:Uid="GridViewDetails" x:Name="GridViewAddToPallet">
                                                <GridViewColumn x:Uid="GridViewColumn_1" Header="Modelo" Width="0" DisplayMemberBinding="{Binding Path= Producto}"/>
                                                <GridViewColumn x:Uid="GridViewColumn_1" Header="Serial" Width="115" DisplayMemberBinding="{Binding Path= Serial}"/>
                                                <GridViewColumn x:Uid="GridViewColumn_1" Header="Mac" Width="115" DisplayMemberBinding="{Binding Path= Mac}"/>
                                                <GridViewColumn x:Uid="GridViewColumn_1" Header="Estado" Width="0" DisplayMemberBinding="{Binding Path= Estado}"/>
                                                <GridViewColumn x:Uid="GridViewColumn_1" Header="Tecnico" Width="115" DisplayMemberBinding="{Binding Path= Tecnico}"/>
                                                <GridViewColumn x:Uid="GridViewColumn_1" Header="" Width="0" DisplayMemberBinding="{Binding Path= RowID}"/>
                                                <GridViewColumn x:Uid="GridViewColumn_1" Header="Usuario última reparación" Width="0" DisplayMemberBinding="{Binding Path= Usuario}"/>
                                            </GridView>
                                        </ListView.View>
                                    </ListView>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock x:Uid="txt_recuento" Text="Registros cargados: " FontWeight="Bold" Margin="0,7,8,4"></TextBlock>
                                        <TextBlock x:Uid="txt_recuentoAdd" x:Name="txt_recuentoAdd" Text="{Binding Path= ListRecordsAddToPallet.Count}" FontWeight="Bold" Margin="0,7,8,4"></TextBlock>
                                    </StackPanel>
                                    <StackPanel x:Uid="Stack_Panel_2" Name="Stack_Panel_222" Width="auto" Orientation="Horizontal" Margin="4,3,4,3" HorizontalAlignment="Right">
                                        <Button x:Uid="btn_remover" Name="btn_remover" Content="Remover" Click="btn_Remove_Click_1"></Button>
                                    </StackPanel>
                                </StackPanel>
                            </Border>
                        </StackPanel>
                    </StackPanel>
                </TabItem>
            </TabControl>
        </DockPanel>
    </AdornerDecorator>
</WMCore:UserControlBase>
