                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Text="Seleccion de Items" HorizontalAlignment="Left" FontWeight="bold" />
                                        <Image x:Uid="ImageRefresh" Name="ImageRefresh"  Margin="10,1,5,0" Width="17" HorizontalAlignment="Center" Source="/WpfFront;component/Images/refresh.png" ToolTip="Refresh List" Visibility="Collapsed" MouseDown="ImageRefresh_MouseDown" />
                                    </StackPanel>
                                    <Separator Margin="5,4,0,0"/>
                                    <StackPanel x:Uid="stkData" Orientation="Horizontal" Margin="0,6,4,4">
                                        <TextBlock x:Uid="TextBlock_4" Width="50" FontWeight="Bold" Margin="0,4,4,0" HorizontalAlignment="Left" Text="Estado" />
                                        <ComboBox x:Uid="cb_BuscarItems" Name="cb_BuscarItems" Width="120" Height="23" ItemsSource="{Binding Path=ListadoPosiciones}" SelectedValue="Code"  SelectionChanged="cb_BuscarItem_SelectionChanged" SelectedIndex="-1">
                                            <ComboBoxItem Content="MAL ESTADO"/>
                                        </ComboBox>
                                        <TextBlock x:Uid="txt_buscarPorTecnico" x:Name="txt_buscarPorTecnico" Width="100" FontWeight="Bold" Margin="100,4,10,0" HorizontalAlignment="Left" Text="Filtrar por técnico encargado" Visibility="Visible" />
                                        <ComboBox x:Uid="cbo_BuscarByTecnico" x:Name="cbo_BuscarByTecnico" Width="170" Height="23" ItemsSource="{Binding Path=ListadoTecnicoReparacion}" DisplayMemberPath="Tecnico" SelectedValue="Tecnico" SelectionChanged="SelectionChanged_FiltrarPorTecnico" Visibility="Visible" />
                                        <Image x:Name="refreshListadoTecnicos"  Margin="10,1,5,0" Width="17"  HorizontalAlignment="Center" Source="/WpfFront;component/Images/refresh.png" ToolTip="Actualice la lista de técnicos" Cursor="Hand" MouseDown="refreshListadoTecnicos_MouseDown"  />
                                        <Button x:Name="btn_BuscarEquipos" Content="Buscar equipos" Margin="90,0,0,0" IsEnabled="False" FontWeight="Bold" Click="btn_BuscarEquipos_Click"></Button>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Left">
                                        <TextBlock x:Name="txt_filterResults" Width="400"></TextBlock>
                                    </StackPanel>
                                    <StackPanel x:Uid="Stack_Adicionales_2" x:Name="Stack_Adicionales_2" Orientation="Vertical" Margin="0,6,0,0">

                                        <ListView x:Uid="lvDocumentMaster" ItemsSource="{Binding Path=ListRecords_1}" Margin="0,4,0,0"  MaxHeight="400" Name="lvDocumentMaster_2" Visibility="Visible">
                                            <ListView.View>
                                                <GridView x:Uid="GridViewDetails" x:Name="GridViewDetails_2">
                                                    <GridViewColumn x:Uid="GridViewColumn_2" Header="Producto" Width="275" DisplayMemberBinding="{Binding Path=Producto}" />
                                                    <GridViewColumn x:Uid="GridViewColumn_2" Header="Serial" Width="150" DisplayMemberBinding="{Binding Path=Serial}" />
                                                    <GridViewColumn x:Uid="GridViewColumn_2" Header="Mac" Width="80" DisplayMemberBinding="{Binding Path=Mac}" />
                                                    <GridViewColumn x:Uid="GridViewColumn_2" Header="Estado" Width="100" DisplayMemberBinding="{Binding Path=Estado}" />
                                                    <GridViewColumn x:Uid="GridViewColumn_2" Header="Diagnostico" Width="100" DisplayMemberBinding="{Binding Path=Diagnostico}" />
                                                    <GridViewColumn x:Uid="GridViewColumn_2" Header="" Width="0" DisplayMemberBinding="{Binding Path=RowID}" />
                                                    <GridViewColumn x:Uid="GridViewColumn_2" Header="Tecnico" Width="150" DisplayMemberBinding="{Binding Path=Tecnico}" />
                                                    <GridViewColumn x:Uid="GridViewColumn_2" Header="" Width="0" DisplayMemberBinding="{Binding Path=Usuario}" />
                                                </GridView>
                                            </ListView.View>
                                        </ListView>
                                        <StackPanel Orientation="Horizontal" Visibility="Visible" Name="StackListaEquipos">
                                            <TextBlock x:Uid="textblock_recuento" Text="Registros Cargados: " FontWeight="Bold" Margin="0,7,8,4" />
                                            <TextBlock x:Uid="textblock_recuento" x:Name="textblock_recuento11" Text="{Binding Path=ListRecords_1.Count}" FontWeight="Bold" Margin="0,7,8,4" />
                                            <Button x:Uid="btn_confirmar_2" Name="btn_AddToList" Content="Agregar al pallet" Click="btn_AddToList_Click_1" Margin="700,20,0,0" />
                                        </StackPanel>
                                    </StackPanel>
                                </StackPanel>
                            </Border>
                            <StackPanel Margin="15,0,0,0" Width="200" Height="10">
                            </StackPanel>
                        </StackPanel>
