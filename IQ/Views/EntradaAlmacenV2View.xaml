                                                        <Button x:Uid="Btn_Eliminar" x:Name="Btn_Eliminar" Width="70" Margin="5,0,0,0" Content="Eliminar" Click="Btn_Eliminar_Click_1" ToolTip="Elimina las filas seleccionadas" />
                                                        <Grid Margin="840,0,0,0">
                                                            <TextBlock x:Uid="textblock_estadoCargueSer" x:Name="textblock_estadoCargueSer" Text="Operación detenida" Margin="0,-17,0,0" />
                                                            <ProgressBar x:Uid="PBar_cargueSer" x:Name="PBar_cargueSer" Margin="0,3,0,3" Width="255" Minimum="0" Maximum="100"/>
                                                            <TextBlock Text="{Binding Value, ElementName=PBar_cargueSer, StringFormat=\{0:0\}%}" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                                        </Grid>
                                                    </StackPanel>
                                                </StackPanel>
                                            </Border>
                                        </GroupBox>

                                        <Expander Grid.Row="1" Margin="0,10,0,0" VerticalAlignment="Stretch" Width="auto" Cursor="Hand">
                                            <Expander.Header>
                                                <Border Background="SeaShell">
                                                    <TextBlock>
                                                          <Run Text="Equipos no almacenados: ("/>
                                                          <Run Text="{Binding List_Nocargue.Count}"/>
                                                          <Run Text=")"/>
                                                    </TextBlock>
                                                </Border>
                                            </Expander.Header>
                                            <Border Margin="0,0,0,0" Background="AliceBlue">
                                                <StackPanel x:Name="stackpanel111">
                                                    <ListView ButtonBase.Click="GridViewColumnHeaderClickedHandler" MouseDoubleClick="ListView_MouseDoubleClick" x:Uid="lv_NoCargue" ItemsSource="{Binding List_Nocargue}" Margin="0,0,0,0"  MaxHeight="280" x:Name="lv_NoCargue">
                                                        <ListView.ItemContainerStyle>
                                                            <Style TargetType="{x:Type ListViewItem}">
                                                                <Setter Property="HorizontalContentAlignment" Value="Center"/>
                                                            </Style>
                                                        </ListView.ItemContainerStyle>
                                                        <ListView.View>
                                                            <GridView x:Uid="GridViewDetails" x:Name="GridViewDetails11">

                                                                <GridViewColumn x:Uid="GridViewColumn_2" Width="180" DisplayMemberBinding="{Binding Serial1}">
                                                                    <GridViewColumn.Header>
                                                                        <TextBlock Text="Serial" ToolTip="Serial" />
                                                                    </GridViewColumn.Header>
                                                                </GridViewColumn>

                                                                <GridViewColumn x:Uid="GridViewColumn_2" Width="160" DisplayMemberBinding="{Binding Mac1}">
                                                                    <GridViewColumn.Header>
                                                                        <TextBlock Text="Mac" ToolTip="Mac" />
                                                                    </GridViewColumn.Header>
                                                                </GridViewColumn>

                                                                <GridViewColumn x:Uid="GridViewColumn_2" Width="160" DisplayMemberBinding="{Binding CodSAP}">
                                                                    <GridViewColumn.Header>
                                                                        <TextBlock Text="Código SAP" ToolTip="Código SAP" />
                                                                    </GridViewColumn.Header>
                                                                </GridViewColumn>

                                                                <GridViewColumn x:Uid="GridViewColumn_2" Width="120" DisplayMemberBinding="{Binding TipoRecibo}">
                                                                    <GridViewColumn.Header>
                                                                        <TextBlock Text="Tipo recibo" ToolTip="Tipo recibo" />
                                                                    </GridViewColumn.Header>
                                                                </GridViewColumn>

                                                                <GridViewColumn x:Uid="GridViewColumn_2" Width="120" DisplayMemberBinding="{Binding TipoOrigen}">
                                                                    <GridViewColumn.Header>
                                                                        <TextBlock Text="Tipo origen" ToolTip="Tipo origen" />
                                                                    </GridViewColumn.Header>
                                                                </GridViewColumn>

                                                                <GridViewColumn x:Uid="GridViewColumn_2" Width="90" DisplayMemberBinding="{Binding Remision}">
                                                                    <GridViewColumn.Header>
                                                                        <TextBlock Text="Remisión" ToolTip="Remisión" />
                                                                    </GridViewColumn.Header>
                                                                </GridViewColumn>

                                                                <GridViewColumn x:Uid="GridViewColumn_2" Width="90" DisplayMemberBinding="{Binding Caja}">
                                                                    <GridViewColumn.Header>
                                                                        <TextBlock Text="Caja" ToolTip="Caja" />
                                                                    </GridViewColumn.Header>
                                                                </GridViewColumn>

                                                                <GridViewColumn x:Uid="GridViewColumn_2" Width="325" DisplayMemberBinding="{Binding Motivo}">
                                                                    <GridViewColumn.Header>
                                                                        <TextBlock Text="Motivo" ToolTip="Motivo del no cargue" />
                                                                    </GridViewColumn.Header>
                                                                </GridViewColumn>
                                                            </GridView>
                                                        </ListView.View>
                                                    </ListView>
                                                    <StackPanel Orientation="Horizontal" Margin="5,10,0,5">
                                                        <Image x:Uid="ImageRefresh" Name="ImageRefresh"  Margin="0,-6,5,0"  HorizontalAlignment="Center" Source="/WpfFront;component/Images/excel2.png" ToolTip="Exportar a archivo de excel" MouseDown="Btn_Exportar_Click_1"/>
                                                        <TextBlock Text="Exportar" HorizontalAlignment="Left" FontWeight="bold" />
                                                    </StackPanel>
                                                </StackPanel>
                                            </Border>
                                        </Expander>
                                    </StackPanel>
                                </StackPanel>
                            </Border>
                        </StackPanel>
                    </StackPanel>
                </TabItem>
            </TabControl>
        </DockPanel>
    </AdornerDecorator>
</WMCore:UserControlBase>