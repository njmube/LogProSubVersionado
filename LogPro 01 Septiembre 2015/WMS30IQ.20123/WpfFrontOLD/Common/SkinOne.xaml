        <Setter x:Uid="Setter_12" Property="TabStripPlacement" Value="Bottom" />
        <Setter x:Uid="Setter_13" Property="IsSynchronizedWithCurrentItem" Value="True" />
        <Setter x:Uid="Setter_14" Property="Height" Value="Auto" />
        <Setter x:Uid="Setter_15" Property="Width" Value="Auto" />
        <Setter x:Uid="Setter_16" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_1" TargetType="{x:Type TabControl}">
                    <Grid x:Uid="Grid_1" ClipToBounds="true" SnapsToDevicePixels="true" KeyboardNavigation.TabNavigation="Local">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition x:Uid="ColumnDefinition0" x:Name="ColumnDefinition0" />
                            <ColumnDefinition x:Uid="ColumnDefinition1" Width="0" x:Name="ColumnDefinition1" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition x:Uid="RowDefinition0" x:Name="RowDefinition0" Height="Auto" />
                            <RowDefinition x:Uid="RowDefinition1" x:Name="RowDefinition1" Height="*" />
                        </Grid.RowDefinitions>
                        <Border x:Uid="ContentPanel" x:Name="ContentPanel" Background="{TemplateBinding Background}" BorderBrush="{DynamicResource BrushDarkBackground}" BorderThickness="{TemplateBinding BorderThickness}" Grid.Column="0" Grid.Row="1" KeyboardNavigation.DirectionalNavigation="Contained" KeyboardNavigation.TabIndex="2" KeyboardNavigation.TabNavigation="Local">
                            <ContentPresenter x:Uid="PART_SelectedContentHost" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" Margin="{TemplateBinding Padding}" x:Name="PART_SelectedContentHost" ContentSource="SelectedContent" />
                        </Border>
                        <Border x:Uid="HeaderPanelBorder" x:Name="HeaderPanelBorder" Margin="0,0,0,0" BorderBrush="{DynamicResource BrushDarkBackground}" Grid.Column="0" Grid.Row="0" BorderThickness="1,0,1,0" Padding="4,0,4,0">
                            <TabPanel x:Uid="HeaderPanel" x:Name="HeaderPanel" IsItemsHost="true" Panel.ZIndex="1" KeyboardNavigation.TabIndex="1" Margin="0,4,0,0" />
                        </Border>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger x:Uid="Trigger_1" Property="TabStripPlacement" Value="Bottom">
                            <Setter x:Uid="Setter_17" Property="Grid.Row" TargetName="HeaderPanelBorder" Value="1" />
                            <Setter x:Uid="Setter_18" Property="Grid.Row" TargetName="ContentPanel" Value="0" />
                            <Setter x:Uid="Setter_19" Property="Height" TargetName="RowDefinition0" Value="*" />
                            <Setter x:Uid="Setter_20" Property="Height" TargetName="RowDefinition1" Value="Auto" />
                            <Setter x:Uid="Setter_21" Property="Margin" TargetName="HeaderPanel" Value="0,0,0,4" />
                        </Trigger>
                        <Trigger x:Uid="Trigger_2" Property="TabStripPlacement" Value="Left">
                            <Setter x:Uid="Setter_22" Property="Grid.Row" TargetName="HeaderPanelBorder" Value="0" />
                            <Setter x:Uid="Setter_23" Property="Grid.Row" TargetName="ContentPanel" Value="0" />
                            <Setter x:Uid="Setter_24" Property="Grid.Column" TargetName="HeaderPanelBorder" Value="0" />
                            <Setter x:Uid="Setter_25" Property="Grid.Column" TargetName="ContentPanel" Value="1" />
                            <Setter x:Uid="Setter_26" Property="Width" TargetName="ColumnDefinition0" Value="Auto" />
                            <Setter x:Uid="Setter_27" Property="Width" TargetName="ColumnDefinition1" Value="*" />
                            <Setter x:Uid="Setter_28" Property="Height" TargetName="RowDefinition0" Value="*" />
                            <Setter x:Uid="Setter_29" Property="Height" TargetName="RowDefinition1" Value="0" />
                            <Setter x:Uid="Setter_30" Property="Margin" TargetName="HeaderPanelBorder" Value="2,2,0,2" />
                        </Trigger>
                        <Trigger x:Uid="Trigger_3" Property="TabStripPlacement" Value="Right">
                            <Setter x:Uid="Setter_31" Property="Grid.Row" TargetName="HeaderPanelBorder" Value="0" />
                            <Setter x:Uid="Setter_32" Property="Grid.Row" TargetName="ContentPanel" Value="0" />
                            <Setter x:Uid="Setter_33" Property="Grid.Column" TargetName="HeaderPanelBorder" Value="1" />
                            <Setter x:Uid="Setter_34" Property="Grid.Column" TargetName="ContentPanel" Value="0" />
                            <Setter x:Uid="Setter_35" Property="Width" TargetName="ColumnDefinition0" Value="*" />
                            <Setter x:Uid="Setter_36" Property="Width" TargetName="ColumnDefinition1" Value="Auto" />
                            <Setter x:Uid="Setter_37" Property="Height" TargetName="RowDefinition0" Value="*" />
                            <Setter x:Uid="Setter_38" Property="Height" TargetName="RowDefinition1" Value="0" />
                            <Setter x:Uid="Setter_39" Property="Margin" TargetName="HeaderPanelBorder" Value="0,2,2,2" />
                        </Trigger>
                        <Trigger x:Uid="Trigger_4" Property="IsEnabled" Value="false">
                            <Setter x:Uid="Setter_40" Property="Foreground" Value="{StaticResource {x:Static SystemColors.GrayTextBrushKey}}" />
