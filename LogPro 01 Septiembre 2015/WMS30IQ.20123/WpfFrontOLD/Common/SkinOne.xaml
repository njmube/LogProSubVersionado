                                <Condition x:Uid="Condition_1" Property="MinWidth" Value="0" SourceName="Bd" />
                                <Condition x:Uid="Condition_2" Property="IsMouseOver" Value="true" SourceName="Bd" />
                            </MultiTrigger.Conditions>
                            <Setter x:Uid="Setter_54" Property="BorderBrush" TargetName="Bd" Value="{DynamicResource BrushToolBarButtonBorder}" />
                            <Setter x:Uid="Setter_55" Property="Background" TargetName="Bd" Value="{DynamicResource BrushTabItemSelected}" />
                        </MultiTrigger>
                        <MultiTrigger x:Uid="MultiTrigger_2">
                            <MultiTrigger.Conditions>
                                <Condition x:Uid="Condition_3" Property="IsSelected" Value="false" />
                                <Condition x:Uid="Condition_4" Property="IsMouseOver" Value="true" />
                            </MultiTrigger.Conditions>
                            <Setter x:Uid="Setter_56" Property="Background" TargetName="Bd" Value="{DynamicResource BrushToolBarMouseOverBackground}" />
                            <Setter x:Uid="Setter_57" Property="BorderBrush" TargetName="Bd" Value="{DynamicResource BrushToolBarButtonBorder}" />
                        </MultiTrigger>
                        <Trigger x:Uid="Trigger_6" Property="TabStripPlacement" Value="Top">
                            <Setter x:Uid="Setter_58" Property="CornerRadius" TargetName="Bd" Value="6,6,0,0" />
                        </Trigger>
                        <Trigger x:Uid="Trigger_7" Property="TabStripPlacement" Value="Bottom">
                            <Setter x:Uid="Setter_59" Property="BorderThickness" TargetName="Bd" Value="1,0,1,1" />
                            <Setter x:Uid="Setter_60" Property="CornerRadius" TargetName="Bd" Value="0,0,6,6" />
                        </Trigger>
                        <Trigger x:Uid="Trigger_8" Property="TabStripPlacement" Value="Left">
                            <Setter x:Uid="Setter_61" Property="BorderThickness" TargetName="Bd" Value="1,1,0,1" />
                        </Trigger>
                        <Trigger x:Uid="Trigger_9" Property="TabStripPlacement" Value="Right">
                            <Setter x:Uid="Setter_62" Property="BorderThickness" TargetName="Bd" Value="0,1,1,1" />
                        </Trigger>
                        <MultiTrigger x:Uid="MultiTrigger_3">
                            <MultiTrigger.Conditions>
                                <Condition x:Uid="Condition_5" Property="IsSelected" Value="true" />
                                <Condition x:Uid="Condition_6" Property="TabStripPlacement" Value="Top" />
                            </MultiTrigger.Conditions>
                            <Setter x:Uid="Setter_63" Property="Margin" Value="0,-2,-2,-1" />
                            <Setter x:Uid="Setter_64" Property="Margin" TargetName="Content" Value="0,0,0,1" />
                        </MultiTrigger>
                        <MultiTrigger x:Uid="MultiTrigger_4">
                            <MultiTrigger.Conditions>
                                <Condition x:Uid="Condition_7" Property="IsSelected" Value="true" />
                                <Condition x:Uid="Condition_8" Property="TabStripPlacement" Value="Bottom" />
                            </MultiTrigger.Conditions>
                            <Setter x:Uid="Setter_65" Property="Margin" Value="0,-1,-2,-2" />
                            <Setter x:Uid="Setter_66" Property="Margin" TargetName="Content" Value="0,1,0,0" />
                            <Setter x:Uid="Setter_67" Property="Margin" TargetName="Bd" Value="-0.309,0,0.309,0" />
                        </MultiTrigger>
                        <MultiTrigger x:Uid="MultiTrigger_5">
                            <MultiTrigger.Conditions>
