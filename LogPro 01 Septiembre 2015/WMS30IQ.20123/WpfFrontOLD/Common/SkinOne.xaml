        <Setter x:Uid="Setter_5" Property="Background" Value="{x:Null}" />
        <Setter x:Uid="Setter_6" Property="HorizontalContentAlignment" Value="Center" />
        <Setter x:Uid="Setter_7" Property="VerticalContentAlignment" Value="Center" />
        <Setter x:Uid="Setter_8" Property="Padding" Value="0,0,0,0" />
        <Setter x:Uid="Setter_9" Property="VerticalContentAlignment" Value="Stretch" />
        <Setter x:Uid="Setter_10" Property="HorizontalContentAlignment" Value="Stretch" />
