<UserControl x:Uid="BinRangeName" x:Class="WpfFront.Common.UserControls.BinRange"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:uc="clr-namespace:WpfFront.Common.UserControls"
    Height="Auto" Width="Auto" x:Name="BinRangeName">

    <StackPanel x:Uid="StackPanel_1">
    <StackPanel x:Uid="StackPanel_2" Orientation="Horizontal">
        
        <TextBox x:Uid="txtBin1" Width="80" ToolTip="Initial Bin Location." Height="22" Margin="0,0,3,0" Name="txtBin1"  />

        <TextBlock x:Uid="TextBlock_1" VerticalAlignment="Center" Text="{StaticResource TO}"></TextBlock>
        
        <TextBox x:Uid="txtBin2" Width="80" ToolTip="Final Bin Location." Height="22" Margin="3,0,3,0" Name="txtBin2"  />
        

        <Button x:Uid="imgLoad" Height="22" MinWidth="22" Name="imgLoad" Margin="4,0,0,0" Click="imgLoad_FocusHandler" GotFocus="imgLoad_FocusHandler" >
            <Image x:Uid="Image_1" HorizontalAlignment="Center" Margin="-2" Focusable="True" Source="/WpfFront;component/Images/arrow.png"   />
        </Button>
        
        
    </StackPanel>
    
</StackPanel>
</UserControl>
