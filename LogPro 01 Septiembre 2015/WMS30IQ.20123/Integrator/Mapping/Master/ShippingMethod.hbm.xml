<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Entities.Master" assembly="Entities">
  <class name="ShippingMethod" table="Master.ShippingMethod">
	<id name="ShpMethodID" type="Int16">
		<generator class="native" />
	</id>
	
	<many-to-one lazy="false" name="Company" class="Company" column="CompanyID" not-null="true" unique-key="ukShipMthd" />
	<property name="Name" type="String" length="50"  unique-key="ukShipMthd"  />
    <property name="ErpCode" type="String" length="50" not-null="true"  />
	<property name="IsFromErp" type="Boolean" not-null="true"  />
    
	<property name="CreatedBy" type="String" length="50" not-null="true"  />
	<property name="CreationDate" type="DateTime" not-null="true"   />
	<property name="CreTerminal" type="String" length="50"   />
	<property name="ModifiedBy" type="String" length="50"   />
	<property name="ModDate" type="DateTime"   />
	<property name="ModTerminal" type="String" length="50"   />
    
  </class>
</hibernate-mapping>