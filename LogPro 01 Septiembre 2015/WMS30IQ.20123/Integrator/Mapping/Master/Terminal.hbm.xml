<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Entities.Master" assembly="Entities">
  <class name="Terminal" table="Master.Terminal">
	<id name="TerminalID" type="Int32">
		<generator class="native" />
	</id>
    <!-- <many-to-one lazy="false" name="Company" class="Company" column="CompanyID" not-null="true" /> -->
    <many-to-one lazy="false" name="Location" class="Location" column="LocationID" not-null="true" unique-key="ukTerminal" />       
    <property name="Name" type="String" not-null="true" length="50" unique-key="ukTerminal" />
	<property name="Description" type="Int32" />
	<many-to-one lazy="false" name="Status" class="Entities.General.Status" column="StatusID" not-null="true" />


	<property name="CreatedBy" type="String" length="50" not-null="true"  />
	<property name="CreationDate" type="DateTime" not-null="true"   />
	<property name="CreTerminal" type="String" length="50"   />
	<property name="ModifiedBy" type="String" length="50"   />
	<property name="ModDate" type="DateTime"   />
	<property name="ModTerminal" type="String" length="50"   />
  </class>
</hibernate-mapping>