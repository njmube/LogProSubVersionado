<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Entities.General" assembly="Entities">
  <class name="PickMethod" table="General.PickMethod">
	<id name="MethodID" type="Int16">
		<generator class="assigned" />
	</id>
	  <property name="Name" type="String" length="50" not-null="false" unique="true" />
	  <property name="Description" type="String" length="100" />
	  <property name="Active" type="Boolean" />
  </class>
</hibernate-mapping>